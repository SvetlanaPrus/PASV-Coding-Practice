//Меньшее из трех чисел

//Дано три числа. Найдите меньшее из них.
//Напишите функцию с именем minOfThree, которая принимает три числа a, b, c в качестве аргументов и 
//возвращает меньшее из этих чисел. Если числа равны, вернуть любое из них.

//Примеры:

//функция minOfThree(5, 15, 18) должна возвратить 5;
//функция minOfThree(20, 9, 11) должна возвратить 9;
//функция minOfThree(20, 9, 2) должна возвратить 2;
//функция minOfThree(3, 3, 8) должна возвратить 3.


function minOfThree(a, b, c){
let min = 0;
  if (a === b && a < c){
    min = a;
  } else if (b === c && b < a){
    min = b;
  } else if (a === c && a < b){
    min = a;
  } else if (a < b && a < c) {
    min = a;
  } else if (b < a && b < c) {
    min = b;
  } else if (c < a && c < b) {
    min = c;
  } else min = a;
  return min;
}
