//Количество положительных чисел в наборе

//Дано три числа. Найдите, сколько среди них положительных чисел.
//Напишите функцию с именем countOfPositive, которая принимает три числа a, b, c в качестве аргументов и возвращает количество положительных чисел в этом наборе.

//Примеры:

//функция countOfPositive(5, 13, 9) должна возвратить 3;
//функция countOfPositive(-2, 7, 9) должна возвратить 2;
//функция countOfPositive(-1, -7, 0) должна возвратить 0.


function countOfPositive(a, b, c){
let count = 0;
let newArray = [a, b, c];
  for (let i = 0; i < newArray.length; i++) {
    if (newArray[i] > 0) {
    count = count + 1;
    }
  }
  return count; 
}
