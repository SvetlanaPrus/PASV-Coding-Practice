//Выведите числа в порядке убывания.

//Дано два числа. Вывести сначала большее, потом меньшее число.
//Напишите функцию с именем putInOrder, которая принимает два числа a, b в качестве аргументов и 
//возвращает сначала большее, потом меньшее число в виде массива [max, min].

//Примеры:

//функция putInOrder(5, 1) должна возвратить [5, 1];
//функция putInOrder(2, 9) должна возвратить [9, 2];
//функция putInOrder(3, 3) должна возвратить [3, 3].


function putInOrder(a, b){
let min = 0;
let max = 0;
let result = [];
  if (a > b) {
    min = b; max = a;
    result.push(max, min);
  } else {
    min = a; max = b;
    result.push(max, min);
  }
  return result;
}
